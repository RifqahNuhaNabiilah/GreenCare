<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <RelativeLayout android:id="@+id/page_utama_ek1"
        android:layout_marginTop="0dp"
        android:layout_marginStart="0dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >


        <ImageView android:id="@+id/_1_1"
            android:src="@drawable/_1_1"
            android:contentDescription="_1_1"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <ImageView android:id="@+id/greencare_1"
            android:src="@drawable/greencare_1"
            android:contentDescription="greencare_1"
            android:layout_marginStart="125dp"
            android:layout_marginTop="127dp"
            android:layout_width="157.31dp"
            android:layout_height="149.74dp" />

        <ImageButton   android:id="@+id/_rectangle_1"
            android:background="@drawable/_rectangle_3_shape"
            android:layout_marginStart="41dp"
            android:layout_marginTop="495dp"
            android:layout_width="327dp"
            android:layout_height="35dp" />

        <ImageButton   android:id="@+id/_rectangle_2"
            android:background="@drawable/_rectangle_4_shape"
            android:layout_marginStart="41dp"
            android:layout_marginTop="545dp"
            android:layout_width="327dp"
            android:layout_height="35dp" />

        <TextView android:id="@+id/login"
            android:textStyle="bold"
            android:layout_gravity="center_horizontal|center"
            android:gravity="center"
            android:textSize="14sp"
            android:textColor="@color/_bg__utama_ek2_color"
            android:layout_marginLeft="138dp"
            android:layout_marginTop="550dp"
            android:layout_width="134.4dp"
            android:layout_height="25.4dp"
            android:text="@string/login_string"
            />

        <TextView android:id="@+id/sign_up"
            android:textStyle="bold"
            android:layout_gravity="center_horizontal|center"
            android:gravity="center"
            android:textSize="14sp"
            android:textColor="@color/_bg__utama_ek2_color"
            android:layout_marginLeft="138dp"
            android:layout_marginTop="500dp"
            android:layout_width="134.4dp"
            android:layout_height="25.4dp"
            android:text="@string/sign_up_string"
            />

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>